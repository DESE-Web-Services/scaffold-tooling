{
  "name": "deptcms/scaffold-tooling",
  "description": "A variety of standard config, scripts and packages to support GovCMS scaffold.",
  "minimum-stability": "dev",
  "repositories": [
    {
      "type": "composer",
      "url": "https://repo.packagist.com/empdev/d8/"
    }
  ],
  "require": {
    "drupal/clamav": "2.0.0",
    "drupal/coder": "8.3.x-dev",
    "drupal/drupal-extension": "4.1.0",
    "drupal/lagoon_logs": "^2",
    "drupal/redis": "1.5.0",
    "drupal/stage_file_proxy": "^1",
    "drush/drush": "^10",
    "phar-io/manifest": "^2",
    "phar-io/version": "^3",
    "webflo/drupal-finder": "1.2",
    "webmozart/path-util": "2.3",
    "zaporylie/composer-drupal-optimizations": "^1.0"
  },
  "bin": [
    "scripts/govcms-audit",
    "scripts/govcms-behat",
    "scripts/govcms-lint",
    "scripts/govcms-lint-distro",
    "scripts/govcms-phpunit",
    "scripts/govcms-prepare-xml",
    "scripts/govcms-ship-shape",
    "scripts/govcms-vet",
    "scripts/govcms-deploy",
    "scripts/govcms-drush",
